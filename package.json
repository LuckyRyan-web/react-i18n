{
  "name": "react-i18n",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:dtpl": "tsc -P .dtpl/tsconfig.json -w",
    "lint": "lint-staged"
  },
  "dependencies": {
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@ryan-liu/eslint-config-react-app": "^0.1.3",
    "@ryan-liu/stylelint-config": "^1.0.4",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "dot-template-types": "^0.3.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "stylelint": "^14.5.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,tsx,vue}": [
      "eslint --fix --quiet"
    ],
    "src/**/*.json": [
      "prettier --write"
    ],
    "*.{scss,less}": [
      "stylelint --fix"
    ]
  }
}
